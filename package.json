{
    "name": "@mogielski/design-forge",
    "version": "0.0.11",
    "description": "Type-safe design system generator with SCSS and TypeScript support",
    "type": "module",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "bin": {
        "design-forge": "./src/cli/index.ts"
    },
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        }
    },
    "files": [
        "src/**/*",
        "static",
        "bin"
    ],
    "scripts": {
        "test": "node --test",
        "test:dev": "node ./src/cli/index.ts init"
    },
    "dependencies": {
        "commander": "^11.0.0",
        "inquirer": "^8.2.5",
        "prettier": "^3.3.3"
    },
    "devDependencies": {
        "@types/inquirer": "^9.0.7",
        "esbuild": "^0.20.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.0.0"
    }
}
