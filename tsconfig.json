{
  "compilerOptions": {
      "target": "ES2018",
      "module": "CommonJS",
      "lib": ["ES2018"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "outDir": "./dist",
      "rootDir": ".",
      "baseUrl": "."
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/static/**"]
}
